<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior" tools:context=".Forum"
    tools:showIn="@layout/app_bar_main" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.android.com/tools ">


    <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent"
        android:orientation="vertical">


        <Button android:onClick="initializeRanglisteButton" android:id="@+id/Rangliste"
            android:layout_width="match_parent" android:layout_height="120dp" android:textAlignment="center"

            android:text="Rangliste" />

        <Button android:onClick="initializeNewGameButton" android:id="@+id/NewGame"
            android:layout_width="match_parent" android:layout_height="120dp"


            android:text="Spiel erfassen" />


        <Button android:id="@+id/Forum" android:layout_width="match_parent" android:layout_height="120dp"


            android:text="Forum" />

        <Button android:id="@+id/Tipps" android:layout_width="match_parent" android:layout_height="120dp"


            android:text="Tipps und Tricks" />


    </LinearLayout>


</android.support.constraint.ConstraintLayout>
